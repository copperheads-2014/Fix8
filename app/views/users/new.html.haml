= form_for @user, :url => '/users', :action => 'create', html: {class: "form-horizontal", role: "form"}  do |f|
  %fieldset
    %legend
      Are you a
      = render partial: 'user_types'
  = render partial: 'form'
  = render partial: 'password_fields'
  %div.form-group
    %div.col-sm-offset-2.col-sm-10
      =submit_tag "Create Account", class: "btn btn-default"
